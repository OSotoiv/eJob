{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobFilter.schema.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "minSalary": {
            "type": "string",
            "minLength": 1,
            "pattern": "^[0-9]+$"
        },
        "hasEquity": {
            "type": "string",
            "enum": [
                "true",
                "false"
            ]
        }
    },
    "additionalProperties": false,
    "required": []
}